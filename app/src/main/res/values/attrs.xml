<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="RatingBarView">
        <attr name="starImageSize" format="dimension"/>
        <attr name="starCount" format="integer"/>
        <attr name="starEmpty" format="reference"/>
        <attr name="starFill" format="reference"/>
    </declare-styleable>

    <declare-styleable name="WaveView">
        <attr name="wave_width" format="dimension"/>
        <attr name="wave_height" format="dimension"/>
        <attr name="wave_ratio" format="float"/>
        <attr name="wave_color" format="color"/>
        <attr name="speed" format="integer"/>

        <attr name="wave_width2" format="dimension"/>
        <attr name="wave_height2" format="dimension"/>
        <attr name="wave_ratio2" format="float"/>
        <attr name="wave_color2" format="color"/>
        <attr name="speed2" format="integer"/>
    </declare-styleable>


    <declare-styleable name="RatioImageView">
        <attr name="ratio" format="float"/>
    </declare-styleable>

    <declare-styleable name="NineGridLayout">
        <attr name="sapcing" format="dimension"/>
    </declare-styleable>
</resources>